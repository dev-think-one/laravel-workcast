{
    "name": "yaroslawww/laravel-workcast",
    "description": "workcast api integration for laravel",
    "keywords": [
        "workcast",
        "workcast.com",
        "laravel-workcast"
    ],
    "homepage": "https://github.com/yaroslawww/laravel-workcast",
    "license": "MIT",
    "authors": [
        {
            "name": "Yaroslav Georgitsa",
            "email": "yaroslav.georgitsa@gmail.com",
            "homepage": "https://github.com/yaroslawww",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">7.4",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^6.16",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelWorkcast\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelWorkcast\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelWorkcast\\ServiceProvider"
            ],
            "aliases": {
                "Workcast": "LaravelWorkcast\\Workcast"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
